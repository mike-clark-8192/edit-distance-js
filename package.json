{
  "name": "edit-distance",
  "version": "1.0.0",
  "description": "JavaScript (TypeScript) port of belambert/edit-distance",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.min.js",
  "browser": "dist/index.iife.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production npm run build:build",
    "build:dev": "cross-env NODE_ENV=development npm run build:build",
    "build:build": "shx rm -rf dist && rollup -c",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/shelljs": "^0.8.15",
    "cross-env": "^7.0.3",
    "dts-buddy": "^0.4.7",
    "jest": "^29.7.0",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.0",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "type-fest": "^4.18.2",
    "typescript": "^5.4.5"
  }
}
